{
  "name": "shiftwise-workforce-planning",
  "version": "1.0.0",
  "description": "AI-assisted workforce planning system for production environments",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:e2e": "vitest --run src/test/e2e",
    "test:e2e:watch": "vitest src/test/e2e",
    "test:performance": "vitest --run src/test/e2e/performance.test.ts",
    "test:security": "vitest --run src/test/e2e/security.test.ts",
    "test:load": "vitest --run src/test/e2e/load-testing.test.ts",
    "test:workflow": "vitest --run src/test/e2e/complete-planning-workflow.test.ts",
    "test:scenarios": "tsx src/test/runners/comprehensive-test-runner.ts run",
    "test:scenarios:list": "tsx src/test/runners/comprehensive-test-runner.ts list",
    "test:scenarios:setup": "tsx src/test/runners/comprehensive-test-runner.ts setup",
    "test:scenarios:cleanup": "tsx src/test/runners/comprehensive-test-runner.ts cleanup",
    "migrate": "tsx src/database/migrate.ts up",
    "migrate:rollback": "tsx src/database/migrate.ts down",
    "migrate:status": "tsx src/database/migrate.ts status",
    "seed": "tsx src/database/seed.ts"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "uuid": "^11.1.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.8",
    "jsdom": "^26.1.0",
    "supertest": "^7.1.4",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
